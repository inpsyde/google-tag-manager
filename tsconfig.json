{
    "compileOnSave": false,
    "compilerOptions": {
        "declaration": false,
        "downlevelIteration": true,
        "esModuleInterop": true,
        "importHelpers": true,
        "jsx": "react",
        "lib": ["dom", "esnext"],
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noLib": false,
        "removeComments": false,
        "resolveJsonModule": true,
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": false,
        "target": "esnext",
    },
    "exclude": ["node_modules", "vendor"]
}